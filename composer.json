{
    "name": "wilsenhc/laravel-rif-validation",
    "description": "A Validation rule to check if a RIF is valid",
    "keywords": [
        "wilsenhc",
        "seniat",
        "rif",
        "laravel-rif-validation",
        "laravel-validar-rif"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Wilsen Hern√°ndez",
            "email": "wilsenforwork@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.3|^8.0",
        "illuminate/support": "^6.0|^7.0|^8.0",
        "illuminate/contracts": "^6.0|^7.0|^8.0"
    },
    "require-dev": {
        "orchestra/testbench": "^4.5|^5.0|^6.0",
        "phpunit/phpunit": "^9.4"
    },
    "autoload": {
        "psr-4": {
            "Wilsenhc\\RifValidation\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Wilsenhc\\RifValidation\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "extra": {
        "laravel" : {
            "providers" : [
                "Wilsenhc\\RifValidation\\RifValidationServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
