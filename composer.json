{
    "name": "wilsenhc/laravel-rif-validation",
    "description": "A Validation rule to check if a RIF is valid",
    "keywords": [
        "wilsenhc",
        "seniat",
        "rif",
        "laravel-rif-validation",
        "laravel-validar-rif"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Wilsen Hern√°ndez",
            "email": "wilsenforwork@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.2",
        "illuminate/support": "^11.0|^12.0",
        "illuminate/contracts": "^11.0|^12.0",
        "wilsenhc/rif-validation": "dev-main"
    },
    "require-dev": {
        "orchestra/testbench": "^9.0|^10.0",
        "phpunit/phpunit": "^11.0|^12.0"
    },
    "autoload": {
        "psr-4": {
            "Wilsenhc\\RifValidation\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Wilsenhc\\RifValidation\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "extra": {
        "laravel" : {
            "providers" : [
                "Wilsenhc\\RifValidation\\RifValidationServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
